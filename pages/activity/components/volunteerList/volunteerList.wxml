<view class="weui-cells__title">Amazing Volunteers</view>
<view class="weui-cells weui-cells_after-title">
  <view class="weui-cell" wx:for="{{volunteers.items}}" wx:key="name">
    <view class="weui-cell__bd">{{item.name}}, {{item.value}}</view>
    <view wx:if='{{item.isChampion}}'>
		  <image src="../../../../images/champion.png" style="margin-right: 16px;vertical-align: middle;width:20px; height: 20px;"></image>
    </view>
  </view>
</view>